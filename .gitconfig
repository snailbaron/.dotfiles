[log]
	decorate = true
[push]
	default = upstream
[alias]
	hist = log --graph --decorate --oneline --all
[core]
	excludesfile = /home/immartynov/.gitignore_global
[color "diff"]
	whitespace = red reverse
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
